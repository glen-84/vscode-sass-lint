{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "../sass-lint/server",
        "newLine": "LF",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true
    },
    "include": [
        "src/sass-lint-server.ts",
        "typings.d.ts"
    ]
}
